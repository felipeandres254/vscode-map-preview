{
    "name": "vscode-map-preview",
    "displayName": "Map Preview",
    "description": "VSCode extension to preview a map",
    "version": "1.0.0",
    "engines": {
        "vscode": "^1.31.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:map.previewLocation"
    ],
    "main": "./src/main",
    "contributes": {
        "commands": [{
            "command": "map.previewLocation",
            "title": "Preview location",
            "category": "Maps"
        }]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/bluebird": "^3.5.26",
        "@types/mocha": "^2.2.42",
        "@types/node": "^10.12.21",
        "bluebird": "^3.5.3",
        "eslint": "^5.13.0",
        "jimp": "^0.6.0",
        "typescript": "^3.3.1",
        "vscode": "^1.1.28"
    }
}
